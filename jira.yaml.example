    verify: True    
  "prod":
    url: "https://prod.atlassian.net"
    rsa_cert_file: "/home/vagrant/jira.pem"
    auth_method: "oauth"
    oauth_token: ""
    oauth_secret: ""
    consumer_key: ""
    project: "MY_PROJECT"
    verify: True